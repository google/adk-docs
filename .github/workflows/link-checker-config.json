{
  "ignorePatterns": [
    {
      "pattern": "^https?://[^/]*localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "{{BASEURL}}/"
    },
    {
      "pattern": "%20",
      "replacement": "-",
      "global": true
    }
  ],
  "timeout": "5s",
  "retryOn429": true,
  "retryCount": 10,
  "fallbackRetryDelay": "5s",
  "aliveStatusCodes": [200, 206]
}
